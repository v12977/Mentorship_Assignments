#include <bits/stdc++.h>
using namespace std;

int findGreatest(int a, int b, int c) {
    int greatest = a;

    if (b > greatest) {
        greatest = b;
    }

    if (c > greatest) {
        greatest = c;
    }

    return greatest;
}

int main() {
    int num1, num2, num3;

    std::cout << "Enter three numbers: ";
    std::cin >> num1 >> num2 >> num3;

    int greatest = findGreatest(num1, num2, num3);

    std::cout << "The greatest number is: " << greatest << std::endl;

    return 0;
}
